TD_tb_button_debouncer.tc_2_1_reset_test ;
    %vpi_call 2 172 "$display", "[TC-2.1] Reset Test - Starting..." {0 0 0};
    %load/vec4 v0x1326513b0_0;
    %addi 1, 0, 32;
    %store/vec4 v0x1326513b0_0, 0, 32;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x132650c70_0, 0, 1;
    %delay 200000, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x132651300_0, 0, 1;
    %delay 100000, 0;
    %load/vec4 v0x132650db0_0;
    %pad/u 32;
    %cmpi/ne 0, 0, 32;
    %jmp/0xz  T_0.0, 6;
    %vpi_call 2 185 "$display", "[TC-2.1] FAIL: btn_out should be 0 during reset, got %b", v0x132650db0_0 {0 0 0};
    %load/vec4 v0x132651080_0;
    %addi 1, 0, 32;
    %store/vec4 v0x132651080_0, 0, 32;
    %jmp T_0.1;
T_0.0 ;
    %vpi_call 2 188 "$display", "[TC-2.1] PASS: btn_out = 0 during reset" {0 0 0};
T_0.1 ;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x132651300_0, 0, 1;
    %delay 200000, 0;
    %load/vec4 v0x132650db0_0;
    %pad/u 32;
    %cmpi/ne 0, 0, 32;
    %jmp/0xz  T_0.2, 6;
    %vpi_call 2 197 "$display", "[TC-2.1] FAIL: btn_out should remain 0 immediately after reset" {0 0 0};
    %load/vec4 v0x132651080_0;
    %addi 1, 0, 32;
    %store/vec4 v0x132651080_0, 0, 32;
    %jmp T_0.3;
T_0.2 ;
    %vpi_call 2 200 "$display", "[TC-2.1] PASS: btn_out remains 0 after reset release" {0 0 0};
    %load/vec4 v0x1326511d0_0;
    %addi 1, 0, 32;
    %store/vec4 v0x1326511d0_0, 0, 32;
T_0.3 ;
    %pushi/vec4 0, 0, 1;
